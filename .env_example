# Django secret key (replace with your secret key)
DJANGO_SECRET_KEY=your-secret-key

# Stripe keys (replace with your Stripe keys)
STRIPE_TEST_SECRET_KEY=your-stripe-secret-key
STRIPE_TEST_PUBLIC_KEY=your-stripe-public-key

# Database settings (replace with your database configuration)
DB_NAME=your-database-name
DB_USER=your-database-user
DB_PASSWORD=your-database-password
DB_HOST=localhost
DB_PORT=5432

# Email configuration (use placeholder values)
EMAIL_BACKEND=django.core.mail.backends.smtp.EmailBackend
EMAIL_HOST=smtp.example.com
EMAIL_PORT=587
EMAIL_HOST_USER=your-email@example.com
EMAIL_HOST_PASSWORD=your-email-password
EMAIL_USE_TLS=True
EMAIL_USE_SSL=False

# Redis settings (replace with your Redis configuration)
REDIS_URL=redis://localhost:6379/0
CELERY_BROKER_URL=redis://localhost:6379/0
CELERY_RESULT_BACKEND=redis://localhost:6379/0

# Celery settings
CELERY_TIMEZONE=UTC
